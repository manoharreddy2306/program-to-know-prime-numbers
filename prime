#include<stdio.h>
int main()
{
        int x, y, hcf;
        printf("Enter the first number:  ");
        scanf("%d",&x);
        printf("Enter the second number:  ");
        scanf("%d",&y);
        int i;
        for(i=1;i<=x;i++)
        {
            if(x%i==0 && y%i==0)
            {
                hcf=1;
            }
        }
        if(hcf==1)
        {
                printf("The Given numbers %d and %d are relatively prime numbers", x, y);
               
        }
        else
        {
                printf("The given numbers %d and %d are not Relatively prime", x, y);
        }
return 0;
}

